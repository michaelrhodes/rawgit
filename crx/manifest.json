{
  "name": "RawGit",
  "description": "Use RawGit to view raw files on Github.",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "bundle.js"
      ],
      "matches": [
        "https://github.com/*",
        "https://gist.github.com/*"
      ]
    }
  ]
}